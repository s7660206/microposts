<h1><%= @title %></h1>
<ol><%= render @users %></ol>

<div class="row">
  <aside class="col-md-4">
    <%= render 'user_info' %>
  </aside>
  <div class="col-md-8">
    <h3><%= @title %> (<%= @users.count %>)</h3>
    <ol class="users">
      <%= render @users %>
    </ol>
    <%= paginate @users %>
  </div>
</div>